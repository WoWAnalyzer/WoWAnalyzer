  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@babel/register": "7.22.5",
    "@babel/types": "7.22.5",
    "@emotion/babel-plugin": "^11.11.0",
    "@lingui/cli": "3.17.2",
    "@lingui/loader": "3.17.2",
    "@martijnhols/eslint-config": "^5.1.0",
    "@playwright/test": "^1.36.2",
    "@sentry/browser": "^7.61.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12",
    "@types/decompress": "^4.2.4",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.11.18",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.6",
    "@types/react-test-renderer": "^16.9.5",
    "@types/react-textfit": "^1.1.0",
    "@types/react-transition-group": "^4.4.6",
    "@types/react-virtualized": "^9.21.22",
    "archiver": "^5.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "decompress": "^4.2.1",
    "eslint-plugin-progress": "^0.0.1",
    "eslint-plugin-wowanalyzer": "link:./packages/eslint-plugin-wowanalyzer",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest-canvas-mock": "^2.5.2",
    "lint-staged": "13.2.3",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "~2.8.1",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.64.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.4"
  },
  "resolutions": {
    "@lingui/cli": "3.17.2",
    "@lingui/core": "3.17.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint-plugin-react": "^7.26.1",
    "prettier-plugin-tailwindcss": "0.1.7"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "safestart": "yarn && react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:interface": "yarn test --testPathIgnorePatterns \"integrationTests|/src\\/parser/\"",
    "test:parser": "yarn test ./src/parser --testPathIgnorePatterns integrationTests",
    "test:integration": "yarn test integrationTests --passWithNoTests",
    "typecheck": "tsc",
    "lint-uncached": "eslint --format stylish --max-warnings 0",
    "lint": "yarn lint-uncached --cache .",
    "lint:fix": "yarn lint --fix",
    "extract": "lingui extract --overwrite --clean",
    "script": "set NODE_ENV=development&&babel-node",
    "postinstall": "patch-package",
    "prepare": "is-ci || husky install",
    "e2e:setup": "node ./scripts/e2e/generate-configs-bootstrap.js",
    "e2e": "yarn e2e:setup && playwright test",
    "e2e:codegen": "playwright codegen http://localhost:3000",
    "e2e:report": "playwright show-report"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,mdx,yml,graphql,scss,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "yarn lint-uncached --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "globalSetup": "./jest.setup.js",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.{spec,test}.{ts,tsx}"
    ],
    "snapshotResolver": "./snapshotResolver.js",
    "moduleNameMapper": {
      "^@lingui/loader!.*$": "<rootDir>/linguiMessageMock.js"
    }
  },
  "packageManager": "yarn@1.22.19"
}
