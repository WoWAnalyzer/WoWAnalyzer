  "license": "AGPL-3.0-or-later",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@lingui/core": "3.14.0",
    "@lingui/macro": "3.14.0",
    "@lingui/react": "3.14.0",
    "@reduxjs/toolkit": "^1.9.1",
    "@types/react-toggle": "^4.0.3",
    "@wowanalyzer/react-tooltip-lite": "^3.1.1",
    "es6-error": "^4.1.1",
    "make-plural": "^6.2.2",
    "prop-types": "^15.7.2",
    "rc-slider": "^10.0.0",
    "react": "^17.0.2",
    "react-animate-height": "^2.0.23",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-masonry-component": "^6.3.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1",
    "react-select-search": "^2.2.4",
    "react-textfit": "^1.1.1",
    "react-toggle": "^4.1.2",
    "react-transition-group": "^1.2.1",
    "react-vega": "^7.4.4",
    "react-virtualized": "^9.22.3",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.0",
    "sass-text-stroke": "1.2.0",
    "swr": "^0.5.7",
    "universal-cookie": "^4.0.4",
    "vega": "~5.21.0",
    "vega-lite": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-proposal-private-property-in-object": "^7.20.5",
    "@emotion/babel-plugin": "^11.10.5",
    "@lingui/cli": "3.14.0",
    "@lingui/loader": "3.14.0",
    "@martijnhols/eslint-config": "^5.1.0",
    "@sentry/browser": "^7.28.1",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^24.9.1",
    "@types/node": "^18.11.18",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.6",
    "@types/react-test-renderer": "^16.9.5",
    "@types/react-virtualized": "^9.21.21",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "archiver": "^5.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "decompress": "^4.2.1",
    "enzyme": "^3.11.0",
    "eslint-plugin-progress": "^0.0.1",
    "eslint-plugin-wowanalyzer": "file:./packages/eslint-plugin-wowanalyzer",
    "husky": "^4.3.8",
    "jest-canvas-mock": "^2.3.1",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "13.1.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "~2.8.1",
    "react-app-rewired": "^2.1.11",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.49.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.2"
  },
  "resolutions": {
    "@lingui/cli": "3.14.0",
    "@lingui/core": "3.14.0",
    "eslint-plugin-react": "^7.26.1",
    "prettier-plugin-tailwindcss": "0.1.7"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "safestart": "yarn && react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:interface": "yarn test --testPathIgnorePatterns \"integrationTests|/src\\/parser/\"",
    "test:parser": "yarn test ./src/parser --testPathIgnorePatterns integrationTests",
    "test:integration": "yarn test integrationTests --passWithNoTests",
    "typecheck": "tsc",
    "lint": "eslint --format stylish --max-warnings 0 --cache .",
    "lint:fix": "yarn lint --fix",
    "extract": "lingui extract --overwrite --clean",
    "script": "set NODE_ENV=development&&babel-node",
    "postinstall": "patch-package"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,mdx,yml,graphql,scss,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "yarn --silent lint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "globalSetup": "./jest.setup.js",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.{spec,test}.{ts,tsx}"
    ],
    "snapshotResolver": "./snapshotResolver.js",
    "moduleNameMapper": {
      "^@lingui/loader!.*$": "<rootDir>/linguiMessageMock.js"
    }
  }
}
