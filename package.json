    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-masonry-component": "^6.3.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.14.2",
    "react-select-search": "^4.1.6",
