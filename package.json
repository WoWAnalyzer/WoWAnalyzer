    "start": "vite",
    "safestart": "yarn && react-app-rewired start",
    "build": "vite build",
    "test": "vitest",
    "test:interface": "yarn test --exclude \"src/parser/**/*\"",
    "test:parser": "yarn test ./src/parser",
    "typecheck": "tsc",
    "lint-uncached": "eslint --format stylish --max-warnings 0",
    "lint": "yarn lint-uncached --cache .",
    "lint:fix": "yarn lint --fix",
    "extract": "lingui extract --overwrite --clean",
    "script": "set NODE_ENV=development&&babel-node",
    "postinstall": "patch-package",
    "prepare": "is-ci || husky install",
    "e2e:setup": "node scripts/e2e/generate-configs-bootstrap.cjs",
    "e2e": "yarn e2e:setup && playwright test",
    "e2e:codegen": "playwright codegen http://localhost:3000",
    "e2e:report": "playwright show-report"
  },
