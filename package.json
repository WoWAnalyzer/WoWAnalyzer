  "resolutions": {
    "@lingui/cli": "3.17.2",
    "@lingui/core": "3.17.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint-plugin-react": "^7.26.1",
    "prettier-plugin-tailwindcss": "0.1.7"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "safestart": "pnpm install && react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:interface": "nr test --testPathIgnorePatterns \"integrationTests|/src\\/parser/\"",
    "test:parser": "nr test ./src/parser --testPathIgnorePatterns integrationTests",
    "test:integration": "nr test integrationTests --passWithNoTests",
    "typecheck": "tsc",
    "lint-uncached": "eslint --format stylish --max-warnings 0",
    "lint": "nr lint-uncached --cache .",
    "lint:fix": "nr lint --fix",
    "extract": "lingui extract --overwrite --clean",
    "script": "set NODE_ENV=development&&babel-node",
    "prepare": "is-ci || husky install",
    "e2e:setup": "node ./scripts/e2e/generate-configs-bootstrap.js",
    "e2e": "nr e2e:setup && playwright test",
    "e2e:codegen": "playwright codegen http://localhost:3000",
    "e2e:report": "playwright show-report"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,mdx,yml,graphql,scss,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "pnpm run lint-uncached --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "globalSetup": "./jest.setup.js",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.{spec,test}.{ts,tsx}"
    ],
    "snapshotResolver": "./snapshotResolver.js",
    "moduleNameMapper": {
      "^@lingui/loader!.*$": "<rootDir>/linguiMessageMock.js"
    }
  },
  "packageManager": "pnpm@8.9.2",
  "pnpm": {
    "patchedDependencies": {
      "vega-embed@6.20.2": "patches/vega-embed@6.20.2.patch",
      "prettier-plugin-tailwindcss@0.1.7": "patches/prettier-plugin-tailwindcss@0.1.7.patch"
    }
  }
}
